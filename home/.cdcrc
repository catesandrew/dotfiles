if [[ $USER == 'vagrant' ]]; then
    CDC_DIRS=($HOME/workflow /vagrant/src)
else
    CDC_DIRS=(
        /usr/local/work/uie
        /usr/local/work/api
        /usr/local/work/devops
        /usr/local/work/lmp
        /usr/local/work/playground
    )
fi

CDC_IGNORE=(training hold save)
CDC_AUTO_PUSH=true
CDC_REPOS_ONLY=true
CDC_COLOR=true
CDC_SUCCESS_COLOR='\033[0;92m'
CDC_WARNING_COLOR='\033[0;93m'
CDC_ERROR_COLOR='\033[0;91m'
